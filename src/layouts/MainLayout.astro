---
import BaseLayout from "./BaseLayout.astro";

type Props = {
  title?: string;
  description?: string;
};

const { title, description } = Astro.props;
---

<BaseLayout title={title} description={description}>
  <div class="flex min-h-svh flex-col bg-slate-100">
    <main class="overflow-hidden">
      <slot />
    </main>
  </div>
</BaseLayout>
