{
  "name": "hackcanadawebsite",
  "type": "module",
  "version": "0.4.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --config .prettierrc.mjs --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
