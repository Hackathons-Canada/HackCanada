  },
  "dependencies": {
    "@astrojs/react": "^3.6.2",
    "@astrojs/tailwind": "^5.1.2",
    "@nanostores/react": "^0.7.3",
    "@playform/compress": "^0.1.4",
    "animejs": "^3.2.2",
    "astro": "^4.16.0",
    "astro-lenis": "^1.0.0",
    "clsx": "^2.1.1",
    "gsap": "^3.12.5",
    "lenis": "^1.1.14",
    "marked": "^14.1.2",
    "nanostores": "^0.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/js": "^9.12.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/animejs": "^3.1.12",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
