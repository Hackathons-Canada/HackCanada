{
  "name": "hackcanadawebsite",
  "type": "module",
  "version": "0.4.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --config .prettierrc.mjs --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/react": "^3.6.3",
    "@astrojs/tailwind": "^5.1.2",
    "@nanostores/react": "^0.7.3",
    "@playform/compress": "^0.1.6",
    "animejs": "^3.2.2",
    "astro": "^4.16.16",
    "astro-lenis": "^1.0.0",
    "clsx": "^2.1.1",
    "gsap": "^3.12.5",
    "lenis": "^1.1.17",
    "marked": "^14.1.4",
    "nanostores": "^0.11.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@eslint/js": "^9.15.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/animejs": "^3.1.12",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
